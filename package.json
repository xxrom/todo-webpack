{
  "name": "todo",
  "version": "0.0.1",
  "description": "Todo",
  "main": "index.js",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "yarn && yarn clean && yarn dev",
    "dev": "NODE_ENV=development webpack-dev-server --hot",
    "dev:sourceMap": "NODE_ENV=development SOURCE_MAP=source-map webpack-dev-server --hot",
    "build": "yarn clean && NODE_ENV=production webpack",
    "webpack:watch": "nodemon --exec 'webpack-dev-server --hot' --watch ./webpack.config.js",
    "tsc-lint": "tsc --noEmit",
    "prettier": "prettier --check './src/**/*.{ts,tsx}'",
    "fix-prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "tslint": "tslint -c tslint.js -p tsconfig.json './src/**/*.{ts,tsx}'",
    "fix-tslint": "tslint --fix -c tslint.js -p tsconfig.json './src/**/*.{ts,tsx}'",
    "fix": "yarn fix-prettier && yarn fix-tslint",
    "clean": "rm -rf .linaria-cache dist node_modules/.cache"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,css,scss}": [
      "yarn fix",
      "yarn tslint",
      "git add"
    ]
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.6.0",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack-env": "^1.13.9",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-hot-loader": "^4.12.5",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.1",
    "@hot-loader/react-dom": "^16.8.6",
    "apollo-boost": "^0.4.3",
    "axios": "^0.19.0",
    "graphql": "^14.4.2",
    "linaria": "^1.3.1",
    "lint-staged": "^9.1.0",
    "react": "^16.8.4",
    "react-apollo": "^2.5.8",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  }
}
